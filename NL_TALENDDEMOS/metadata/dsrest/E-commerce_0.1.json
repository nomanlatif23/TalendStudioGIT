    "/Advanced" : {
      "get" : {
        "summary" : "Search for matching customers",
        "responses" : {
          "200" : {
            "description" : "Status 200",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/Customer"
                  }
                }
              }
            }
          },
          "404" : {
            "description" : "Status 404"
          }
        }
      }
    },
    "/order/{number}" : {
      "get" : {
        "summary" : "Get Order",
        "responses" : {
          "200" : {
            "description" : "Status 200",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/SalesOrder"
                }
              }
            }
          },
          "400" : {
            "description" : "Status 400",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object"
                }
              }
            }
          }
        }
      },
      "delete" : {
        "summary" : "Delete Order",
        "responses" : {
          "200" : {
            "description" : "Status 200"
          }
        }
      },
      "parameters" : [ {
        "name" : "number",
        "in" : "path",
        "required" : true,
        "schema" : {
